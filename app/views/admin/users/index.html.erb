<h1>All Users</h1>

<table>
  <thead>
    <th>Username</th>
    <th>First Name</th>
    <th>Last Name</th>
    <th>Role</th>
  </thead>
  <% @users.each do |user| %>
    <tr>
      <td><%= user.username %></td>
      <td><%= user.first_name %></td>
      <td><%= user.last_name %></td>
      <td><%= user.role %></td>
      
    </tr>
  <% end %>
</table>
