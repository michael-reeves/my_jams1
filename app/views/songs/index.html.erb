<p>
  <strong>Newest song:</strong> <%= session[:most_recent_song_title] %>
</p>

<h1>All Songs</h1>

<% @songs.each do |song| %>
  <!-- <%= link_to song.title, song_path(song) %><br> -->
  <%= link_to song.title, song %>
  <%= link_to "Edit", edit_song_path(song) %>
  <%= link_to "Delete", song_path(song), method: :delete, data: {confirm: "Are You Sure?"} %>
  <br>
<% end %>

<%= link_to "New Song", new_song_path %>
